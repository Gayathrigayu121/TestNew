<?xml version="1.0" encoding="UTF-8"?>
  <extentreports>
<configuration>
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>STANDARD</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>

    <!-- title of the document -->
    <documentTitle>UrbanSDK</documentTitle>

   <!-- timestamp format -->
		<timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

    <!-- report name - displayed at top-nav -->
    <reportName>
    <![CDATA[
            <img src='.\src\test\resources\Logos\jman.jpg' />
        ]]>              
    </reportName>


    <!-- location of charts in the test view -->
    <!-- top, bottom -->
    <testViewChartLocation>top</testViewChartLocation>

    <!-- custom javascript -->
    <scripts>
        <![CDATA[
            $(document).ready(function() {

            });
        ]]>
    </scripts>

    <!-- custom styles -->
    <styles>
        <![CDATA[
            .report-name { padding-left: 18px; } .report-name > img { float: 
      left;height: 30%;margin-left: 30px;margin-top: 3px;width: auto; }
        ]]>
    </styles>
</configuration>
</extentreports>